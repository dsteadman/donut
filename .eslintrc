{
    "parser": "babel-eslint",
    "globals": {
        "_": false,
        "PAGEPAYLOAD": false
    },
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
        "quotes": [ 2, "single" ],
        "linebreak-style": [ 2, "unix" ],
        "semi": [ 2, "always" ],
        "no-underscore-dangle": 0,
        "indent": [ 2, 2 ],
        "strict": ['error' , 'never'],
        "no-extra-strict": 0,
        "space-infix-ops": 0,
        "no-console": 0,
        "no-use-before-define": 0,
        "consistent-return": 0,
        "no-extra-bind": 0,
        "no-shadow": 0
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
    },
    "plugins": [
        "react"
    ]
}
